#!groovy
properties([disableConcurrentBuilds()])

pipeline {
    agent any
    triggers { pollSCM('* * * * *') }
    options {
        buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
        timestamps()
    }
    stages {
        stage("first step") {
            steps {
                echo("1 steeeep")
                sh 'cd Downloads'
                sh 'selenium-side-runner test_jenkins.side'
            }
        }
    }
}
